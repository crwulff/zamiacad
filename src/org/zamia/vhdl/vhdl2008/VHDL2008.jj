AssociationElement association_element()  throws ZamiaException :
{
	AssociationElement ae;
	Operation op;
	FormalPart fp = null;
}
{
	[ LOOKAHEAD( formal_part() <FOLLOWS> ) fp = formal_part() <FOLLOWS>  ]
