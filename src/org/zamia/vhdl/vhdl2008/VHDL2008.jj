/**
 *
 * for error recovery:
 *
 */
JAVACODE void error_skipto(int kind, String message, ParseException e)
{
  erm.addError (new ZamiaException (message+"\n"+e, new SourceLocation(sf, getLocation(e.currentToken.next))));
  Token t;
  do
  {
    t = getNextToken();
  } while ((t.kind != kind) && t.kind != EOF);
}
